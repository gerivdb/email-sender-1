{
  "serviceStatus": {
    "enabled": true,
    "refreshInterval": 60,
    "metrics": [
      {
        "id": "running",
        "name": "État du service",
        "description": "Indique si n8n est en cours d'exécution",
        "type": "boolean",
        "source": "pid"
      },
      {
        "id": "uptime",
        "name": "Temps de fonctionnement",
        "description": "Durée depuis le dernier démarrage de n8n",
        "type": "duration",
        "source": "pid"
      },
      {
        "id": "port",
        "name": "Port accessible",
        "description": "Indique si le port n8n est accessible",
        "type": "boolean",
        "source": "port"
      },
      {
        "id": "api",
        "name": "API accessible",
        "description": "Indique si l'API n8n est accessible",
        "type": "boolean",
        "source": "api"
      }
    ]
  },
  "performance": {
    "enabled": true,
    "refreshInterval": 30,
    "metrics": [
      {
        "id": "responseTime",
        "name": "Temps de réponse",
        "description": "Temps de réponse de l'API n8n",
        "type": "duration",
        "source": "api",
        "unit": "ms",
        "history": true,
        "historyPoints": 20
      },
      {
        "id": "memory",
        "name": "Utilisation mémoire",
        "description": "Utilisation de la mémoire par le processus n8n",
        "type": "size",
        "source": "process",
        "unit": "MB",
        "history": true,
        "historyPoints": 20
      },
      {
        "id": "cpu",
        "name": "Utilisation CPU",
        "description": "Utilisation du CPU par le processus n8n",
        "type": "percentage",
        "source": "process",
        "unit": "%",
        "history": true,
        "historyPoints": 20
      }
    ]
  },
  "workflows": {
    "enabled": true,
    "refreshInterval": 300,
    "metrics": [
      {
        "id": "totalWorkflows",
        "name": "Workflows totaux",
        "description": "Nombre total de workflows",
        "type": "number",
        "source": "api"
      },
      {
        "id": "activeWorkflows",
        "name": "Workflows actifs",
        "description": "Nombre de workflows actifs",
        "type": "number",
        "source": "api"
      },
      {
        "id": "inactiveWorkflows",
        "name": "Workflows inactifs",
        "description": "Nombre de workflows inactifs",
        "type": "number",
        "source": "api"
      },
      {
        "id": "workflowsWithErrors",
        "name": "Workflows avec erreurs",
        "description": "Nombre de workflows avec des erreurs",
        "type": "number",
        "source": "api"
      }
    ]
  },
  "history": {
    "enabled": true,
    "refreshInterval": 3600,
    "metrics": [
      {
        "id": "lastRestart",
        "name": "Dernier redémarrage",
        "description": "Date et heure du dernier redémarrage de n8n",
        "type": "datetime",
        "source": "log"
      },
      {
        "id": "restartCount",
        "name": "Nombre de redémarrages",
        "description": "Nombre de redémarrages de n8n dans les dernières 24 heures",
        "type": "number",
        "source": "log"
      },
      {
        "id": "errorCount",
        "name": "Nombre d'erreurs",
        "description": "Nombre d'erreurs dans les logs de n8n dans les dernières 24 heures",
        "type": "number",
        "source": "log"
      },
      {
        "id": "lastError",
        "name": "Dernière erreur",
        "description": "Date, heure et message de la dernière erreur dans les logs de n8n",
        "type": "text",
        "source": "log"
      }
    ]
  },
  "endpoints": {
    "enabled": true,
    "refreshInterval": 300,
    "metrics": [
      {
        "id": "healthz",
        "name": "Endpoint /healthz",
        "description": "État de l'endpoint /healthz",
        "type": "boolean",
        "source": "api",
        "endpoint": "/healthz"
      },
      {
        "id": "rest",
        "name": "Endpoint /rest",
        "description": "État de l'endpoint /rest",
        "type": "boolean",
        "source": "api",
        "endpoint": "/rest/workflows"
      },
      {
        "id": "webhook",
        "name": "Endpoint /webhook",
        "description": "État de l'endpoint /webhook",
        "type": "boolean",
        "source": "api",
        "endpoint": "/webhook"
      }
    ]
  }
}
