                  {
                      "parameters":  {
                                         "jsCode":  "// Fusion des contacts LOT1 avec les lieux\n\n// Récupérer les contacts de la Phase 1\nconst contacts = $input.item(1).json.results || [];\n\n// Formater les contacts avec les informations nécessaires\nconst contactsFormated = contacts.map(contact =\u003e {\n  const properties = contact.properties;\n  \n  // Extraire les informations pertinentes\n  const name = properties.Name?.title[0]?.text?.content || \u0027Contact sans nom\u0027;\n  const email = properties.Email?.email || \u0027\u0027;\n  const venue = properties.Venue?.rich_text[0]?.text?.content || \u0027Salle non spécifiée\u0027;\n  const region = properties.Region?.select?.name || \u0027Région non spécifiée\u0027;\n  const status = properties.Status?.status?.name || \u0027Nouveau\u0027;\n  \n  // Ne garder que les contacts avec email et qui n\u0027ont pas déjà été contactés\n  if (!email || status !== \u0027Nouveau\u0027) {\n    return null;\n  }\n  \n  return {\n    id: contact.id,\n    name,\n    email,\n    venue,\n    region,\n    status\n  };\n}).filter(contact =\u003e contact !== null);\n\n// Retourner les contacts formatés\nreturn {\n  json: {\n    contacts: contactsFormated,\n    count: contactsFormated.length\n  }\n};"
                                     },
                      "id":  "84ff84d6-4b23-4169-aae4-2dfd77d22cc8",
                      "name":  "Fusion LOT1 + Lieux",
                      "type":  "n8n-nodes-base.code",
                      "typeVersion":  2,
                      "position":  [
                                       1460,
                                       500
                                   ]
                  },
                  {
                      "parameters":  {
                                         "jsCode":  "// Fusion du message IA avec les contacts\n\n// Récupérer le message généré par l\u0027IA\nconst messageIA = $input.item(0).json.message || \u0027\u0027;\n\n// Récupérer les contacts formatés\nconst contacts = $input.item(1).json.contacts || [];\n\n// Créer un item pour chaque contact avec le message IA\nreturn contacts.map(contact =\u003e ({\n  json: {\n    ...contact,\n    messageIA\n  }\n}));"
                                     },
                      "id":  "94ff84d6-4b23-4169-aae4-2dfd77d22cc9",
                      "name":  "Fusion IA + Contacts",
                      "type":  "n8n-nodes-base.code",
                      "typeVersion":  2,
                      "position":  [
                                       1680,
                                       400
                                   ]
                  },
                  {
                      "parameters":  {
                                         "jsCode":  "// Personnalisation du message pour chaque contact\n\n// Récupérer les données du contact et le message IA\nconst contact = $json;\nconst messageIA = $json.messageIA || \u0027\u0027;\nconst name = $json.name || \u0027Responsable\u0027;\nconst venue = $json.venue || \u0027votre salle\u0027;\n\n// Personnaliser le message\nconst messagePersonnalise = `Bonjour ${name},\n\n${messageIA}\n\nAu plaisir d\u0027échanger avec vous,\nL\u0027équipe Gribitch\n`;\n\n// Retourner le message personnalisé avec les informations du contact\nreturn {\n  json: {\n    ...contact,\n    message: messagePersonnalise,\n    to: $json.email,\n    subject: `Proposition – Groupe en tournée (automatisé)`\n  }\n};"
                                     },
                      "id":  "f8aa20be-3218-4144-9b82-23592378def7",
