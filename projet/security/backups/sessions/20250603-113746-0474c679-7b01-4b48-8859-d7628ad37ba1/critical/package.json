{
  "name": "error-pattern-analyzer",
  "version": "0.0.1",
  "description": "Analyseur hybride Go/TypeScript pour la détection de patterns d’erreur.",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "test": "npm run compile && node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/error-pattern-analyzer.git"
  },
  "keywords": [
    "error",
    "pattern",
    "analyzer",
    "go",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "chalk": "^4.1.2",
    "eslint": "^8.0.0",
    "typescript": "^4.8.0"
  },
  "displayName": "Error Pattern Analyzer",
  "contributes": {
    "commands": [
      {
        "command": "errorPatternAnalyzer.runAnalysis",
        "title": "Analyser les patterns d’erreur"
      }
    ]
  },
  "engines": {
    "vscode": ">=1.70.0"
  },
  "categories": [
    "Other"
  ]
}
