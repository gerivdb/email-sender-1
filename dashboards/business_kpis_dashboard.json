{
  "title": "Tableau de bord des KPIs métier",
  "description": "Vue d'ensemble des indicateurs clés de performance métier",
  "refresh": "1h",
  "time": {
    "from": "now-30d",
    "to": "now"
  },
  "panels": [
    {
      "id": "business_overview",
      "title": "Vue d'ensemble métier",
      "type": "stat-panel",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 3
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "kpi": "EMAIL_DELIVERABILITY_INDEX",
          "label": "Délivrabilité"
        },
        {
          "kpi": "EMAIL_ENGAGEMENT_INDEX",
          "label": "Engagement"
        },
        {
          "kpi": "EMAIL_BUSINESS_VALUE_INDEX",
          "label": "Valeur métier"
        }
      ]
    },
    {
      "id": "efficacite_panel",
      "title": "Efficacité",
      "type": "section",
      "gridPos": {
        "x": 0,
        "y": 3,
        "w": 24,
        "h": 1
      }
    },
    {
      "id": "delivery_rate_panel",
      "title": "Taux de livraison des emails",
      "type": "gauge",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 8,
        "h": 8
      },
      "options": {
        "min": 0,
        "max": 100,
        "thresholds": [
          { "value": 0, "color": "red" },
          { "value": 90, "color": "orange" },
          { "value": 95, "color": "green" }
        ]
      },
      "targets": [
        {
          "kpi": "EMAIL_DELIVERY_RATE",
          "label": "Taux de livraison (%)"
        }
      ]
    },
