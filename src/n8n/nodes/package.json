{
  "name": "n8n-nodes-mcp",
  "version": "0.1.0",
  "description": "n8n nodes for Model Context Protocol (MCP) integration",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "mcp",
    "model context protocol",
    "ai",
    "memory"
  ],
  "license": "MIT",
  "homepage": "https://example.com",
  "author": {
    "name": "MCP Team",
    "email": "info@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/example/n8n-nodes-mcp.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js src"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MCPClientApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/MCP.node.js",
      "dist/nodes/MCPMemory.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.18",
    "@types/request-promise-native": "~1.0.18",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "axios": "^1.1.3"
  }
}
