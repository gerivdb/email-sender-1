        {
          "id": "key_metrics",
          "title": "Métriques clés",
          "type": "metrics_summary",
          "metrics": [
            {
              "id": "email_delivery_rate_avg",
              "name": "Taux de livraison des emails moyen",
              "metric": "EMAIL_DELIVERY_RATE",
              "function": "avg",
              "format": "{value}%"
            },
            {
              "id": "email_open_rate_avg",
              "name": "Taux d'ouverture des emails moyen",
              "metric": "EMAIL_OPEN_RATE",
              "function": "avg",
              "format": "{value}%"
            },
            {
              "id": "email_click_rate_avg",
              "name": "Taux de clic des emails moyen",
              "metric": "EMAIL_CLICK_RATE",
              "function": "avg",
              "format": "{value}%"
            },
            {
              "id": "conversion_rate_avg",
              "name": "Taux de conversion moyen",
              "metric": "CONVERSION_RATE",
              "function": "avg",
              "format": "{value}%"
            },
            {
              "id": "roi_avg",
              "name": "ROI moyen",
              "metric": "ROI",
              "function": "avg",
              "format": "{value}%"
            }
          ]
        },
        {
          "id": "email_delivery_analysis",
          "title": "Analyse du taux de livraison des emails",
          "type": "chart",
          "chart_type": "line",
          "metric": "EMAIL_DELIVERY_RATE",
          "options": {
            "title": "Taux de livraison des emails",
            "x_axis_label": "Temps",
            "y_axis_label": "Taux de livraison (%)",
            "include_avg_line": true,
            "include_threshold_lines": true,
            "thresholds": [
              {
                "value": 95,
