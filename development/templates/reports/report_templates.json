{
  "templates": [
    {
      "id": "system_performance_report",
      "name": "Rapport de performance système",
      "description": "Rapport détaillé sur la performance du système",
      "type": "system",
      "format": "html",
      "sections": [
        {
          "id": "summary",
          "title": "Résumé",
          "type": "text",
          "content": "Ce rapport présente une analyse détaillée de la performance du système pour la période {start_date} à {end_date}."
        },
        {
          "id": "key_metrics",
          "title": "Métriques clés",
          "type": "metrics_summary",
          "metrics": [
            {
              "id": "cpu_avg",
              "name": "Utilisation CPU moyenne",
              "metric": "CPU",
              "function": "avg",
              "format": "{value}%"
            },
            {
              "id": "cpu_max",
              "name": "Utilisation CPU maximale",
              "metric": "CPU",
              "function": "max",
              "format": "{value}%"
            },
            {
              "id": "memory_avg",
              "name": "Utilisation mémoire moyenne",
              "metric": "Memory",
              "function": "avg",
              "format": "{value}%"
            },
            {
              "id": "memory_max",
              "name": "Utilisation mémoire maximale",
              "metric": "Memory",
              "function": "max",
              "format": "{value}%"
            },
            {
              "id": "disk_avg",
              "name": "Utilisation disque moyenne",
              "metric": "Disk",
              "function": "avg",
              "format": "{value}%"
            }
          ]
        },
        {
          "id": "cpu_analysis",
          "title": "Analyse CPU",
          "type": "chart",
          "chart_type": "line",
          "metric": "CPU",
          "options": {
            "title": "Utilisation CPU",
            "x_axis_label": "Temps",
            "y_axis_label": "Utilisation (%)",
            "include_avg_line": true,
            "include_threshold_lines": true,
            "thresholds": [
              {
                "value": 70,
                "color": "#FADE2A",
                "label": "Avertissement"
              },
              {
                "value": 90,
                "color": "#F2495C",
                "label": "Critique"
              }
            ]
          }
        },
        {
          "id": "memory_analysis",
          "title": "Analyse mémoire",
          "type": "chart",
          "chart_type": "line",
          "metric": "Memory",
          "options": {
            "title": "Utilisation mémoire",
            "x_axis_label": "Temps",
            "y_axis_label": "Utilisation (%)",
            "include_avg_line": true,
            "include_threshold_lines": true,
            "thresholds": [
              {
                "value": 70,
                "color": "#FADE2A",
                "label": "Avertissement"
              },
              {
                "value": 90,
                "color": "#F2495C",
                "label": "Critique"
              }
            ]
          }
        },
        {
          "id": "disk_analysis",
          "title": "Analyse disque",
          "type": "chart",
          "chart_type": "line",
          "metric": "Disk",
          "options": {
            "title": "Utilisation disque",
            "x_axis_label": "Temps",
            "y_axis_label": "Utilisation (%)",
            "include_avg_line": true,
            "include_threshold_lines": true,
            "thresholds": [
              {
                "value": 70,
                "color": "#FADE2A",
                "label": "Avertissement"
              },
              {
                "value": 90,
                "color": "#F2495C",
                "label": "Critique"
              }
            ]
          }
        },
        {
          "id": "anomalies",
          "title": "Anomalies détectées",
          "type": "anomalies",
          "metrics": ["CPU", "Memory", "Disk"],
          "threshold": 2.0,
          "max_anomalies": 5
        },
        {
          "id": "recommendations",
          "title": "Recommandations",
          "type": "recommendations",
          "based_on": ["CPU", "Memory", "Disk"]
        }
      ]
    },
    {
      "id": "application_performance_report",
      "name": "Rapport de performance application",
      "description": "Rapport détaillé sur la performance de l'application",
      "type": "application",
      "format": "html",
      "sections": [
        {
          "id": "summary",
          "title": "Résumé",
          "type": "text",
          "content": "Ce rapport présente une analyse détaillée de la performance de l'application pour la période {start_date} à {end_date}."
        },
        {
          "id": "key_metrics",
          "title": "Métriques clés",
          "type": "metrics_summary",
          "metrics": [
            {
              "id": "response_time_avg",
              "name": "Temps de réponse moyen",
              "metric": "ResponseTime",
              "function": "avg",
              "format": "{value} ms"
            },
            {
              "id": "response_time_p95",
              "name": "Temps de réponse (95ème percentile)",
              "metric": "ResponseTime",
              "function": "percentile",
              "percentile": 95,
              "format": "{value} ms"
            },
            {
              "id": "error_rate_avg",
              "name": "Taux d'erreur moyen",
              "metric": "ErrorRate",
              "function": "avg",
              "format": "{value}%"
            },
            {
              "id": "throughput_avg",
              "name": "Débit moyen",
              "metric": "Throughput",
              "function": "avg",
              "format": "{value} req/s"
            },
            {
              "id": "active_users_avg",
              "name": "Utilisateurs actifs moyens",
              "metric": "ActiveUsers",
              "function": "avg",
              "format": "{value}"
            }
          ]
        },
        {
          "id": "response_time_analysis",
          "title": "Analyse du temps de réponse",
          "type": "chart",
          "chart_type": "line",
          "metric": "ResponseTime",
          "options": {
            "title": "Temps de réponse",
            "x_axis_label": "Temps",
            "y_axis_label": "Temps de réponse (ms)",
            "include_avg_line": true,
            "include_threshold_lines": true,
            "thresholds": [
              {
                "value": 1000,
                "color": "#FADE2A",
                "label": "Avertissement"
              },
              {
                "value": 3000,
                "color": "#F2495C",
                "label": "Critique"
              }
            ]
          }
        },
        {
          "id": "error_rate_analysis",
          "title": "Analyse du taux d'erreur",
          "type": "chart",
          "chart_type": "line",
          "metric": "ErrorRate",
          "options": {
            "title": "Taux d'erreur",
            "x_axis_label": "Temps",
            "y_axis_label": "Taux d'erreur (%)",
            "include_avg_line": true,
            "include_threshold_lines": true,
            "thresholds": [
              {
                "value": 5,
                "color": "#FADE2A",
                "label": "Avertissement"
              },
              {
                "value": 10,
                "color": "#F2495C",
                "label": "Critique"
              }
            ]
          }
        },
        {
          "id": "throughput_analysis",
          "title": "Analyse du débit",
          "type": "chart",
          "chart_type": "line",
          "metric": "Throughput",
          "options": {
            "title": "Débit",
            "x_axis_label": "Temps",
            "y_axis_label": "Requêtes par seconde",
            "include_avg_line": true
          }
        },
        {
          "id": "active_users_analysis",
          "title": "Analyse des utilisateurs actifs",
          "type": "chart",
          "chart_type": "line",
          "metric": "ActiveUsers",
          "options": {
            "title": "Utilisateurs actifs",
            "x_axis_label": "Temps",
            "y_axis_label": "Nombre d'utilisateurs",
            "include_avg_line": true
          }
        },
        {
          "id": "anomalies",
          "title": "Anomalies détectées",
          "type": "anomalies",
          "metrics": ["ResponseTime", "ErrorRate", "Throughput"],
          "threshold": 2.0,
          "max_anomalies": 5
        },
        {
          "id": "recommendations",
          "title": "Recommandations",
          "type": "recommendations",
          "based_on": ["ResponseTime", "ErrorRate", "Throughput"]
        }
      ]
    },
    {
      "id": "business_performance_report",
      "name": "Rapport de performance métier",
      "description": "Rapport détaillé sur les indicateurs de performance métier",
      "type": "business",
      "format": "html",
      "sections": [
        {
          "id": "summary",
          "title": "Résumé",
          "type": "text",
          "content": "Ce rapport présente une analyse détaillée des indicateurs de performance métier pour la période {start_date} à {end_date}."
