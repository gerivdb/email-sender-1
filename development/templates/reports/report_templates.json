                "label": "Avertissement"
              },
              {
                "value": 90,
                "color": "#F2495C",
                "label": "Critique"
              }
            ]
          }
        },
        {
          "id": "email_engagement_analysis",
          "title": "Analyse de l'engagement des emails",
          "type": "chart",
          "chart_type": "line",
          "metrics": ["EMAIL_OPEN_RATE", "EMAIL_CLICK_RATE"],
          "options": {
            "title": "Engagement des emails",
            "x_axis_label": "Temps",
            "y_axis_label": "Taux (%)",
            "include_avg_line": true
          }
        },
        {
          "id": "conversion_analysis",
          "title": "Analyse du taux de conversion",
          "type": "chart",
          "chart_type": "line",
          "metric": "CONVERSION_RATE",
          "options": {
            "title": "Taux de conversion",
            "x_axis_label": "Temps",
            "y_axis_label": "Taux de conversion (%)",
            "include_avg_line": true,
            "include_threshold_lines": true,
            "thresholds": [
              {
                "value": 1,
                "color": "#FADE2A",
                "label": "Avertissement"
              },
              {
                "value": 0.5,
                "color": "#F2495C",
                "label": "Critique"
              }
            ]
          }
        },
        {
          "id": "roi_analysis",
          "title": "Analyse du ROI",
          "type": "chart",
          "chart_type": "line",
          "metric": "ROI",
          "options": {
            "title": "Retour sur investissement",
            "x_axis_label": "Temps",
            "y_axis_label": "ROI (%)",
            "include_avg_line": true,
            "include_threshold_lines": true,
            "thresholds": [
              {
                "value": 200,
                "color": "#FADE2A",
                "label": "Avertissement"
              },
              {
                "value": 100,
                "color": "#F2495C",
                "label": "Critique"
              }
            ]
          }
        },
        {
          "id": "anomalies",
          "title": "Anomalies détectées",
          "type": "anomalies",
          "metrics": ["EMAIL_DELIVERY_RATE", "EMAIL_OPEN_RATE", "EMAIL_CLICK_RATE", "CONVERSION_RATE"],
          "threshold": 2.0,
          "max_anomalies": 5
        },
        {
          "id": "recommendations",
          "title": "Recommandations",
          "type": "recommendations",
          "based_on": ["EMAIL_DELIVERY_RATE", "EMAIL_OPEN_RATE", "EMAIL_CLICK_RATE", "CONVERSION_RATE"]
        }
      ]
    }
  ]
}
