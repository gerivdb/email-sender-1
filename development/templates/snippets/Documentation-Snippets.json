{
  "PowerShell Function Documentation": {
    "prefix": "doc-function",
    "body": [
      "<#",
      ".SYNOPSIS",
      "    ${1:Brief description of the function}",
      "",
      ".DESCRIPTION",
      "    ${2:Detailed description of the function}",
      "",
      ".PARAMETER ${3:ParameterName}",
      "    ${4:Description of the parameter}",
      "",
      ".EXAMPLE",
      "    ${5:Example-Command -Parameter 'Value'}",
      "    ${6:Description of what this example does}",
      "",
      ".NOTES",
      "    Author: ${7:Author Name}",
      "    Date: ${9:$(Get-Date -Format 'yyyy-MM-dd')}",
      "    Version: ${8:1.0}",
      "#>"
    ],
    "description": "PowerShell function documentation block"
  },
  "PowerShell Module Documentation": {
    "prefix": "doc-module",
    "body": [
      "<#",
      ".SYNOPSIS",
      "    ${1:Brief description of the module}",
      "",
      ".DESCRIPTION",
      "    ${2:Detailed description of the module}",
      "",
      ".NOTES",
      "    Module Name: ${3:ModuleName}",
      "    Author: ${4:Author Name}",
      "    Date: ${7:$(Get-Date -Format 'yyyy-MM-dd')}",
      "    Version: ${5:1.0}",
      "",
      ".LINK",
      "    ${6:Project URL or documentation link}",
      "#>"
    ],
    "description": "PowerShell module documentation block"
  },
  "PowerShell Script Documentation": {
    "prefix": "doc-script",
    "body": [
      "<#",
      ".SYNOPSIS",
      "    ${1:Brief description of the script}",
      "",
      ".DESCRIPTION",
      "    ${2:Detailed description of the script}",
      "",
      ".PARAMETER ${3:ParameterName}",
      "    ${4:Description of the parameter}",
      "",
      ".EXAMPLE",
      "    ${5:./Script.ps1 -Parameter 'Value'}",
      "    ${6:Description of what this example does}",
      "",
      ".NOTES",
      "    Author: ${7:Author Name}",
      "    Date: ${9:$(Get-Date -Format 'yyyy-MM-dd')}",
      "    Version: ${8:1.0}",
      "#>"
    ],
    "description": "PowerShell script documentation block"
  },
  "Markdown Documentation Header": {
    "prefix": "doc-md-header",
    "body": [
      "# ${1:Title}",
      "",
      "*Version ${2:1.0} - ${13:$(Get-Date -Format 'yyyy-MM-dd')}*",
      "",
      "${3:Brief description}",
      "",
      "## Table of Contents",
      "",
      "1. [Overview](#overview)",
      "2. [Installation](#installation)",
      "3. [Usage](#usage)",
      "4. [Examples](#examples)",
      "5. [Parameters](#parameters)",
      "6. [Notes](#notes)",
      "",
      "## Overview",
      "",
      "${4:Detailed overview of the module/script/function}",
      "",
      "## Installation",
      "",
      "```powershell",
      "${5:Installation instructions}",
      "```",
      "",
      "## Usage",
      "",
      "```powershell",
      "${6:Usage examples}",
      "```",
      "",
      "## Examples",
      "",
      "### Example 1: ${7:Basic usage}",
      "",
      "```powershell",
      "${8:Example code}",
      "```",
      "",
      "${9:Example explanation}",
      "",
      "## Parameters",
      "",
      "### -${10:ParameterName}",
      "",
      "${11:Parameter description}",
      "",
      "## Notes",
      "",
      "${12:Additional notes and information}"
    ],
    "description": "Markdown documentation header for PowerShell modules/scripts"
  },
  "Markdown Function Documentation": {
    "prefix": "doc-md-function",
    "body": [
      "## ${1:Function-Name}",
      "",
      "${2:Brief description of the function}",
      "",
      "### Syntax",
      "",
      "```powershell",
      "${1:Function-Name} [-${3:Parameter1} <${4:Type}>] [-${5:Parameter2} <${6:Type}>] [<CommonParameters>]",
      "```",
      "",
      "### Description",
      "",
      "${7:Detailed description of the function}",
      "",
      "### Parameters",
      "",
      "#### -${3:Parameter1}",
      "",
      "${8:Description of Parameter1}",
      "",
      "```powershell",
      "Type: ${4:Type}",
      "Parameter Sets: (All)",
      "Aliases: None",
      "Required: True",
      "Position: 0",
      "Default value: None",
      "Accept pipeline input: False",
      "Accept wildcard characters: False",
      "```",
      "",
      "#### -${5:Parameter2}",
      "",
      "${9:Description of Parameter2}",
      "",
      "```powershell",
      "Type: ${6:Type}",
      "Parameter Sets: (All)",
      "Aliases: None",
      "Required: False",
      "Position: 1",
      "Default value: None",
      "Accept pipeline input: False",
      "Accept wildcard characters: False",
      "```",
      "",
      "### Examples",
      "",
      "#### Example 1: ${10:Basic usage}",
      "",
      "```powershell",
      "${11:Example code}",
      "```",
      "",
      "${12:Example explanation}",
      "",
      "### Notes",
      "",
      "${13:Additional notes and information}"
    ],
    "description": "Markdown documentation for a PowerShell function"
  },
  "Markdown Module Documentation": {
    "prefix": "doc-md-module",
    "body": [
      "# ${1:ModuleName}",
      "",
      "*Version ${2:1.0} - ${24:$(Get-Date -Format 'yyyy-MM-dd')}*",
      "",
      "${3:Brief description of the module}",
      "",
      "## Table of Contents",
      "",
      "1. [Overview](#overview)",
      "2. [Installation](#installation)",
      "3. [Functions](#functions)",
      "4. [Examples](#examples)",
      "5. [Requirements](#requirements)",
      "6. [Notes](#notes)",
      "",
      "## Overview",
      "",
      "${4:Detailed overview of the module}",
      "",
      "## Installation",
      "",
      "```powershell",
      "${5:Installation instructions}",
      "```",
      "",
      "## Functions",
      "",
      "The module exports the following functions:",
      "",
      "- [${6:Function1}](#${6:Function1}): ${7:Brief description of Function1}",
      "- [${8:Function2}](#${8:Function2}): ${9:Brief description of Function2}",
      "",
      "### ${6:Function1}",
      "",
      "${7:Brief description of Function1}",
      "",
      "```powershell",
      "${6:Function1} [-${10:Parameter1} <${11:Type}>] [-${12:Parameter2} <${13:Type}>] [<CommonParameters>]",
      "```",
      "",
      "### ${8:Function2}",
      "",
      "${9:Brief description of Function2}",
      "",
      "```powershell",
      "${8:Function2} [-${14:Parameter1} <${15:Type}>] [-${16:Parameter2} <${17:Type}>] [<CommonParameters>]",
      "```",
      "",
      "## Examples",
      "",
      "### Example 1: ${18:Basic usage}",
      "",
      "```powershell",
      "${19:Example code}",
      "```",
      "",
      "${20:Example explanation}",
      "",
      "## Requirements",
      "",
      "- PowerShell ${21:5.1} or later",
      "- ${22:Other requirements}",
      "",
      "## Notes",
      "",
      "${23:Additional notes and information}"
    ],
    "description": "Markdown documentation for a PowerShell module"
  },
  "Python Function Documentation": {
    "prefix": "doc-py-function",
    "body": [
      "def ${1:function_name}(${2:parameters}):",
      "    \"\"\"${3:Brief description of the function}",
      "",
      "    ${4:Detailed description of the function}",
      "",
      "    Args:",
      "        ${5:param_name} (${6:param_type}): ${7:Parameter description}",
      "",
      "    Returns:",
      "        ${8:return_type}: ${9:Description of the return value}",
      "",
      "    Raises:",
      "        ${10:exception_type}: ${11:When this exception is raised}",
      "",
      "    Examples:",
      "        >>> ${1:function_name}(${12:example_param})",
      "        ${13:example_output}",
      "    \"\"\"",
      "    ${0}"
    ],
    "description": "Python function documentation using Google style docstrings"
  },
  "Python Class Documentation": {
    "prefix": "doc-py-class",
    "body": [
      "class ${1:ClassName}:",
      "    \"\"\"${2:Brief description of the class}",
      "",
      "    ${3:Detailed description of the class}",
      "",
      "    Attributes:",
      "        ${4:attr_name} (${5:attr_type}): ${6:Attribute description}",
      "",
      "    Examples:",
      "        >>> obj = ${1:ClassName}(${7:example_param})",
      "        >>> obj.${8:method}()",
      "        ${9:example_output}",
      "    \"\"\"",
      "",
      "    def __init__(self, ${10:parameters}):",
      "        \"\"\"Initialize the ${1:ClassName}",
      "",
      "        Args:",
      "            ${11:param_name} (${12:param_type}): ${13:Parameter description}",
      "        \"\"\"",
      "        ${0}"
    ],
    "description": "Python class documentation using Google style docstrings"
  },
  "Python Module Documentation": {
    "prefix": "doc-py-module",
    "body": [
      "\"\"\"${1:Brief description of the module}",
      "",
      "${2:Detailed description of the module}",
      "",
      "Examples:",
      "    >>> import ${3:module_name}",
      "    >>> ${3:module_name}.${4:function_name}(${5:example_param})",
      "    ${6:example_output}",
      "",
      "Attributes:",
      "    ${7:module_level_variable} (${8:type}): ${9:Description}",
      "\"\"\"",
      "",
      "${0}"
    ],
    "description": "Python module documentation using Google style docstrings"
  }
}
