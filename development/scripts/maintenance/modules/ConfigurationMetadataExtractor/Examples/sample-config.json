{
    "application": {
        "name": "ConfigAnalyzer",
        "version": "1.0.0",
        "environment": "development",
        "debug": true
    },
    "server": {
        "host": "localhost",
        "port": 8080,
        "ssl": {
            "enabled": true,
            "certificate": "certs/server.crt",
            "key": "certs/server.key"
        },
        "timeout": 30,
        "dependsOn": ["database", "logging"]
    },
    "database": {
        "type": "mysql",
        "host": "${server.host}",
        "port": 3306,
        "name": "myapp",
        "user": "dbuser",
        "password": "dbpassword",
        "connectionString": "Server=${database.host};Port=${database.port};Database=${database.name};User Id=${database.user};Password=${database.password};",
        "pool": {
            "min": 5,
            "max": 100,
            "idle": 10000
        },
        "depends_on": "storage"
    },
    "logging": {
        "level": "INFO",
        "file": "logs/app.log",
        "console": true,
        "format": "${timestamp} [${level}] ${message}"
    },
    "storage": {
        "type": "local",
        "path": "/data",
        "backup": {
            "enabled": true,
            "schedule": "0 0 * * *",
            "retention": 7
        }
    },
    "email": {
        "enabled": true,
        "host": "smtp.example.com",
        "port": 587,
        "secure": true,
        "auth": {
            "user": "user@example.com",
            "pass": "password"
        },
        "from": "noreply@example.com"
    },
    "api": {
        "baseUrl": "https://api.example.com",
        "version": "v1",
        "timeout": 5000,
        "retry": {
            "attempts": 3,
            "delay": 1000
        },
        "endpoints": [
            {
                "name": "users",
                "path": "/users",
                "methods": ["GET", "POST", "PUT", "DELETE"],
                "requiresAuth": true
            },
            {
                "name": "products",
                "path": "/products",
                "methods": ["GET"],
                "requiresAuth": false
            }
        ]
    },
    "features": {
        "auth": true,
        "admin": true,
        "reporting": false,
        "notifications": {
            "email": true,
            "sms": false,
            "push": true
        }
    },
    "performance": {
        "cache": {
            "enabled": true,
            "ttl": 3600,
            "maxSize": 1000
        },
        "compression": true,
        "minify": true
    },
    "externalConfig": "config/external.json",
    "configPath": "config/settings.json"
}
